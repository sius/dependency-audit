<% let i = 1;
	licenseGroups.forEach( (g) => { -%>
	<% g.dependencies.forEach( (d) => { 
		const p = d.package[0];
		%>
<%-i++ %>. <%- d.fullname %>

	- Authors:                    (Source: <%- p.file %>)
		<%- p.metadata.authors %>

	- Owners:                     (Source: <%- p.file %>)
		<%- p.metadata.owners %>
	<% if (p.metadata.copyright) { -%>	
  - Copyright:                  (Source: <%- p.file %>)
		<%- p.metadata.copyright %>
	<% } -%>

	- Project-Url:                (Source: <%- p.file %>)
		<%- p.metadata.projectUrl %>

	- License-Url:                (Source: <%- p.file %>)
		<%- p.metadata.licenseUrl %>

	- License Text:

		---> TODO: Insert License text <---
	<% }); -%>
<% }); -%>
